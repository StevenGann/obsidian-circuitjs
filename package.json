{
	"name": "circuitjs",
	"version": "1.1.0",
	"description": "Embed interactive CircuitJS circuit simulations in your notes",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"build:circuitjs": "node scripts/build-circuitjs.mjs",
		"build:full": "npm run build:circuitjs && npm run build",
		"postinstall": "git submodule update --init --recursive",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint ."
	},
	"keywords": [
		"obsidian",
		"obsidian-plugin",
		"circuitjs",
		"circuit",
		"simulation",
		"electronics"
	],
	"author": "Steven Gann",
	"license": "MIT",
	"devDependencies": {
		"@eslint/js": "^9.18.0",
		"@types/node": "^16.11.6",
		"esbuild": "^0.25.0",
		"eslint": "^9.18.0",
		"eslint-plugin-obsidianmd": "^0.1.9",
		"globals": "^15.14.0",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.21.0"
	},
	"dependencies": {
		"adm-zip": "^0.5.16",
		"lz-string": "^1.5.0",
		"obsidian": "latest"
	}
}
